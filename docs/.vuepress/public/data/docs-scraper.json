{
    "index_uid": "vuepress",
    "start_urls": ["https://codenoob.top/"],
    "sitemap_urls": ["https://codenoob.top/sitemap.xml"],
    "stop_urls": ["https://codenoob.top/rss.xml", "https://codenoob.top/tag", "https://codenoob.top/category", "https://codenoob.top/404.html"],
    "selectors": {
        "lvl0": {
            "selector": ".sidebar-heading.open span",
            "global": true,
            "default_value": "Documentation"
        },
        "lvl1": ".page-title h1 span",
        "lvl2": ".theme-default-content h2",
        "lvl3": ".theme-default-content h3",
        "lvl4": ".theme-default-content h4",
        "lvl5": ".theme-default-content h5",
        "lvl6": ".theme-default-content h6",
        "text": ".theme-default-content p, .theme-default-content li"
    },
    "strip_chars": " .,;:#",
    "scrap_start_urls": true,
    "custom_settings": {
        "synonyms": {
            "relevancy": ["relevant", "relevance"],
            "relevant": ["relevancy", "relevance"],
            "relevance": ["relevancy", "relevant"]
        }
    }
}
